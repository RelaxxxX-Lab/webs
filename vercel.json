{
  "version": 2,
  "builds": [
    {
      "src": "api.js",
      "use": "@vercel/node"
    },
    {
      "src": "auth.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/auth.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "BOT_TOKEN": "@bot_token",
    "SERVER_ID": "@server_id",
    "MONGODB_KEYS_URL": "@mongodb_keys_url",
    "BOT_CLIENT_ID": "@bot_client_id",
    "BOT_CLIENT_SECRET": "@bot_client_secret",
    "BOT_PUBLIC_KEY": "@bot_public_key"
  }
}
